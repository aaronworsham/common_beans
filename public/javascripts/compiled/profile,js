(function() {
  /*
  = require application
  = require models/profile_models
  = require views/profile_views
  = require collections/profile_collections
  */  $(function() {
    return $('.follow-user').click(function() {
      var friend;
      friend = new Friend({
        id: window.friendId
      });
      return friend.save();
    });
  });
}).call(this);
