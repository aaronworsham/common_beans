(function() {
  /*
  = require jquery
  = require jquery-ui
  = require jquery.facebox
  = require backbone
  = require backbone_patching
  = require date_formatter
  = require models/profile_models
  = require views/profile_views
  = require collections/profile_collections
  = require logo
  */  $(function() {
    return $('.follow-user').click(function() {
      var friend;
      friend = new Friend({
        id: window.friendId
      });
      return friend.save();
    });
  });
}).call(this);
