<tr id="holding-{{ id }}-today">
  <td>{{ relative_day }}</td>
  <td>totals</td>
  <td>{{ net_shares }}</td>
  <td>{{ todays_price }}(+{{ price_delta }}/day1)</td>
  <td>{{ net_investment }}</td>
  <td>{{ net_return }}</td>
  <td>Today</td>
  <td></td>
</tr>
<tr>
  <td colspan=2></td>
  <td colspan=2>Today's Value: ${{ todays_value }} ({{ todays_price }}*{{ net_shares }})</td>
  <td colspan=4>Total Gain: ${{ total_value_gain }} ({{todays_value}} + {{net_return}} - {{net_investment}})</td>
</tr>
<tr>
  <td colspan=2></td>
  <td>Day Δ {{ day_delta }}</td>
  <td>Week Δ {{ week_delta }}</td>
  <td>Month Δ {{ month_delta }}</td>
  <td>Year Δ {{ year_delta }}</td>
  <td colspan=2></td>
</tr>
