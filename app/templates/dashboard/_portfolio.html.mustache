<div class="portfolio-content" id="{{name}}">
  <table>
  <tr>
  <td>
  <h3>{{name}}</h3>
  </td>
  </tr>
  </table>

  <table>
  <tr>
  <td>
  <h4>Stocks</h4>
  </td>
  </tr>
  </table>

  <table id='portfolio-stock-holding-{{ id }}'>
    <thead>
      <tr class='super_head'>
        <th></th>
        <th colspan=3>Totals</th>
        <th colspan=2>Today's</th>
        <th colspan=3>Gain/Loss</th>
      </tr>
      <tr class='sub_head'>
        <th>Sym</th>
        <th scope="col">Shares</th>
        <th scope="col">Investment</th>
        <th scope="col">Return</th>
        <th scope="col">Price</th>
        <th scope="col">Value</th>
        <th scope="col">Total Gain</th>
      </tr>
    </thead>
  </table>

  <table>
  <tr>
  <td>
  <h4>Funds</h4>
  </td>
  </tr>
  </table>


