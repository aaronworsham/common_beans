<div class='portfolio-content' id="portfolio-content-{{id}}">
<h3>{{name}}</h3>
  <div class='overview'>
    <p>This is the overview </p>
    <p>Add:
      <a href='#' class='add-stock' data-portfolio-id={{id}}>Stock </a> |
      <a href='#' class='add-bond' data-portfolio-id={{id}}>Bond </a> |
      <a href='#' class='add-fund' data-portfolio-id={{id}}>Fund </a> |
      <a href='#' class='add-etf' data-portfolio-id={{id}}>ETF </a> |
      <a href='#' class='add-cd' data-portfolio-id={{id}}>CD </a> |
      <a href='#' class='add-401k' data-portfolio-id={{id}}>401k </a> |
      <a href='#' class='add-ira' data-portfolio-id={{id}}>IRA</a>
  </div>

  <table class='stock-content' id='portfolio-stock-holding-{{ id }}'>
    <thead>
      <tr><th colspan=7>Stocks</td></tr>
      <tr>
        <th>Symbol</th>
        <th>Shares</th>
        <th>Investment</th>
        <th>Return</th>
        <th>Price</th>
        <th>Value</th>
        <th>Gain</th>
      </tr>
    </thead>
  </table>

  <table class='fund-content' id='portfolio-fund-holding-{{ id }}'>
    <thead>
      <tr><th colspan=7>Funds</td></tr>
      <tr>
        <th>Symbol</th>
        <th>Shares</th>
        <th>Investment</th>
        <th>Return</th>
        <th>Price</th>
        <th>Value</th>
        <th>Gain</th>
      </tr>
    </thead>
  </table>

  <table class='etf-content' id='portfolio-etf-holding-{{ id }}'>
    <thead>
      <tr><th colspan=7>ETFs</td></tr>
      <tr>
        <th>Symbol</th>
        <th>Shares</th>
        <th>Investment</th>
        <th>Return</th>
        <th>Price</th>
        <th>Value</th>
        <th>Gain</th>
      </tr>
    </thead>
  </table>


  <table class='bond-content' id='portfolio-bond-holding-{{ id }}'>
    <thead>
      <tr><th colspan=7>Bonds</td></tr>
      <tr>
        <th>Name</th>
        <th>Face Value</th>
        <th>Today's Value</th>
        <th>Gain</th>
      </tr>
    </thead>
  </table>

  <table class='cd-content' id='portfolio-cd-holding-{{ id }}'>
    <thead>
      <tr><th colspan=7>CDs</td></tr>
      <tr>
        <th>Name</th>
        <th>Face Value</th>
        <th>Today's Value</th>
        <th>Gain</th>
      </tr>
    </thead>
  </table>

</div>