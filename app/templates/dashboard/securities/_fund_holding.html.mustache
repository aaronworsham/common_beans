<tr id="holding-{{ id }}-today">
  <td>{{ ticker_symbol }}</td>
  <td>{{ net_units }}</td>
  <td>{{ net_investment }}</td>
  <td>{{ net_return }}</td>
  <td>{{ todays_price }}</td>
  <td>{{ todays_value }}</td>
  <td>{{ total_gain }}</td>
  <td>
    <a href='#' class='buy-fund-event' data-holding-id={{id}}>Bought Units |</a>
    <a href='#' class='sell-fund-event' data-holding-id={{id}}>Sold Units |</a>
    <a href='#' class='edit-fund-holding' data-holding-id={{id}}>Edit |</a>
    <a href='#' class='delete-fund-holding' data-holding-id={{id}}>Remove</a>
  </td>
</tr>
