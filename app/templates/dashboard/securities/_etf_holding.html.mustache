<tr class='holding-header' id="etf-holding-{{ id }}" data-holding-id={{id}} data-holding-type='etf'>
  <td>S</td>
  <td>{{ ticker_symbol }}</td>
  <td>{{ net_units }}</td>
  <td>{{ net_investment }}</td>
  <td>{{ net_return }}</td>
  <td>{{ todays_price }}</td>
  <td>{{ todays_value }}</td>
  <td>{{ total_value_gain }}</td>
</tr>
<tr>
  <td colspan=8 class='holding-drawer'>
    <div class='etf-events' id="etf-events-for-holding-{{ id }}" data-etf-holding-id={{id}} style='display:none'>
      <a href='#' class='add-event' data-holding-id={{id}} data-security-name='etf' data-event-type='buy'>Bought Shares |</a>
      <a href='#' class='add-event' data-holding-id={{id}} data-security-name='etf' data-event-type='sell'>Sold Shares |</a>
      <a href='#' class='edit-etf-olding' data-holding-id={{id}}>Edit |</a>
      <a href='#' class='delete-etf-holding' data-holding-id={{id}}>Remove</a>
      <ul class='etf-buys' id="etf-buys-for-holding-{{ id }}" data-etf-holding-id={{id}}>
        <li>Started with {{starting_units}} units at {{starting_price}} on {{purchased_on}}</li>
      </ul>
      <ul class='etf-sells' id="etf-sells-for-holding-{{ id }}" data-etf-holding-id={{id}}>
      </ul>
    </div>
  </td>
</tr>
